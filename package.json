{
	"name": "oneunlimited",
	"displayName": "OneUnlimited",
	"description": "Autonomous multi-agent system with unlimited capabilities seamlessly integrated in your IDE for full-stack development with secure permission controls",
	"version": "1.0.0",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "One Unlimited Dev Team"
	},
	"license": "Apache-2.0",
	"publisher": "one-unlimited",
	"repository": {
		"type": "git",
		"url": "https://github.com/oneunlimited/oneunlimited"
	},
	"homepage": "https://oneunlimited.dev",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"oneunlimited",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"ai"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "oneunlimited-ActivityBar",
					"title": "OneUnlimited",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"oneunlimited-ActivityBar": [
				{
					"type": "webview",
					"id": "oneunlimited.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "oneunlimited.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "oneunlimited.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(extensions)"
			},
			{
				"command": "oneunlimited.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "oneunlimited.popoutButtonClicked",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "oneunlimited.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "oneunlimited.openInNewTab",
				"title": "Open In New Tab",
				"category": "OneUnlimited"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "oneunlimited.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == oneunlimited.SidebarProvider"
				},
				{
					"command": "oneunlimited.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == oneunlimited.SidebarProvider"
				},
				{
					"command": "oneunlimited.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == oneunlimited.SidebarProvider"
				},
				{
					"command": "oneunlimited.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == oneunlimited.SidebarProvider"
				},
				{
					"command": "oneunlimited.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == oneunlimited.SidebarProvider"
				}
			]
		},
		"configuration": {
			"title": "OneUnlimited",
			"properties": {
				"oneunlimited.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "The vendor of the language model (e.g. copilot)"
						},
						"family": {
							"type": "string",
							"description": "The family of the language model (e.g. gpt-4)"
						}
					},
					"description": "Settings for VSCode Language Model API"
				},
				"oneunlimited.mcp.mode": {
					"type": "string",
					"enum": [
						"full",
						"server-use-only",
						"off"
					],
					"enumDescriptions": [
						"Enable all MCP functionality (server use and build instructions)",
						"Enable MCP server use only (excludes instructions about building MCP servers)",
						"Disable all MCP functionality"
					],
					"default": "full",
					"description": "Controls MCP inclusion in prompts reduces token usage if you only need access to certain functionality."
				},
				"oneunlimited.enableCheckpoints": {
					"type": "boolean",
					"default": true,
					"description": "Enables extension to save checkpoints of workspace throughout the task. Uses git under the hood which may not work well with large workspaces."
				},
				"oneunlimited.disableBrowserTool": {
					"type": "boolean",
					"default": false,
					"description": "Disables extension from spawning browser session."
				},
				"oneunlimited.modelSettings.o3Mini.reasoningEffort": {
					"type": "string",
					"enum": [
						"low",
						"medium",
						"high"
					],
					"default": "medium",
					"description": "Controls the reasoning effort when using the o3-mini model. Higher values may result in more thorough but slower responses."
				},
				"oneunlimited.chromeExecutablePath": {
					"type": "string",
					"default": null,
					"description": "Path to Chrome executable for browser use functionality. If not set the extension will attempt to find or download it automatically."
				},
				"oneunlimited.preferredLanguage": {
					"type": "string",
					"enum": [
						"English",
						"Arabic - العربية",
						"Portuguese - Português (Brasil)",
						"Czech - Čeština",
						"French - Français",
						"German - Deutsch",
						"Hindi - हिन्दी",
						"Hungarian - Magyar",
						"Italian - Italiano",
						"Japanese - 日本語",
						"Korean - 한국어",
						"Polish - Polski",
						"Portuguese - Português (Portugal)",
						"Russian - Русский",
						"Simplified Chinese - 简体中文",
						"Spanish - Español",
						"Traditional Chinese - 繁體中文",
						"Turkish - Türkçe"
					],
					"default": "English",
					"description": "The language that One Unlimited should use for communication."
				},
				"oneunlimited.mcpMarketplace.enabled": {
					"type": "boolean",
					"default": true,
					"description": "Controls whether the MCP Marketplace is enabled."
				},
				"oneunlimited.conversationTelemetry": {
					"type": "boolean",
					"default": false,
					"description": "Share message data code and more extensive telemetry. This data may be used to improve prompts used in One Unlimited train models and understand failure states more accurately."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run build:webview && npm run check-types && npm run lint && node esbuild.js --production",
		"compile:agents": "node esbuild.js --production",
		"test:agents": "npm run compile:agents && node dist/agents/test-agents.js",
		"test:demo-agents": "node src/core/agents/demo-agents.js",
		"test:interactive-agents": "node src/core/agents/interactive-agents.js",
		"test:agent-system": "node scripts/test-agent-system.js",
		"test:llm-agents": "node scripts/run-llm-agents.js",
		"update:api-keys": "node scripts/update-api-keys.js",
		"compile-tests": "tsc -p ./tsconfig.test.json --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts && eslint webview-ui/src --ext ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"test": "vscode-test",
		"install:all": "npm install && cd webview-ui && npm install",
		"dev:webview": "cd webview-ui && npm run dev",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"build:runtime": "docker build -f docker/runtime.Dockerfile -t oneunlimited-runtime ."
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.12",
		"@types/chai": "^5.0.1",
		"@types/diff": "^5.2.1",
		"@types/json-schema": "^7.0.15",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/should": "^11.2.0",
		"@types/sinon": "^17.0.4",
		"@types/vscode": "^1.98.0",
		"@typescript-eslint/eslint-plugin": "^8.27.0",
		"@typescript-eslint/parser": "^8.27.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.1",
		"chai": "^4.3.10",
		"esbuild": "^0.25.0",
		"eslint": "^8.57.0",
		"husky": "^9.1.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"should": "^13.2.3",
		"sinon": "^19.0.2",
		"typescript": "^5.8.2",
		"vscode": "^1.1.37",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.12.4",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.6.4",
		"@aws-sdk/client-bedrock-runtime": "^3.758.0",
		"@google-cloud/vertexai": "^1.9.3",
		"@google/generative-ai": "^0.18.0",
		"@mistralai/mistralai": "^1.5.0",
		"@modelcontextprotocol/sdk": "^1.7.0",
		"@opentelemetry/api": "^1.4.1",
		"@opentelemetry/exporter-trace-otlp-http": "^0.39.1",
		"@opentelemetry/resources": "^1.30.1",
		"@opentelemetry/sdk-node": "^0.39.1",
		"@opentelemetry/sdk-trace-node": "^1.30.1",
		"@opentelemetry/semantic-conventions": "^1.30.0",
		"@types/clone-deep": "^4.0.4",
		"@types/get-folder-size": "^3.0.4",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/pdf-parse": "^1.1.4",
		"@types/turndown": "^5.0.5",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.8.2",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"firebase": "^11.2.0",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"ignore": "^7.0.3",
		"isbinaryfile": "^5.0.2",
		"jsonwebtoken": "^9.0.2",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"node-fetch": "^2.7.0",
		"ollama": "^0.5.13",
		"open-graph-scraper": "^6.9.0",
		"openai": "^4.83.0",
		"os-name": "^6.0.0",
		"p-timeout": "^6.1.4",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"posthog-node": "^4.8.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"strip-ansi": "^7.1.0",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.24.2"
	}
}
